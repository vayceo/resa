<?xml version="1.0" encoding="utf-8"?>
<layer-list
xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@android:id/background"
        android:height="8.0dip">
        <shape>
            <gradient android:startColor="#FF1B1B0D"
                      android:endColor="#FF1B1B0D"
                      android:angle="270.0"
                      android:centerY="1.0"
                      android:centerColor="#FF1B1B0D" />
            <corners android:bottomRightRadius="@dimen/_6sdp"
                     android:bottomLeftRadius="@dimen/_6sdp"/>
        </shape>
</item>
<item android:id="@android:id/progress"
        android:height="8.0dip">
        <clip>
            <shape android:shape="rectangle">
                <gradient android:startColor="#FFC44A49"
                          android:endColor="#FFBD4143"
                          android:angle="180.0"
                          android:type="linear" />
                <corners android:bottomRightRadius="@dimen/_5sdp"
                         android:bottomLeftRadius="@dimen/_5sdp"/>
            </shape>
        </clip>
</item>
</layer-list>